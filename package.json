{
  "name": "rearguard",
  "version": "2.3.3",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "lib",
    "templates"
  ],
  "bin": {
    "rearguard": "./lib/bin/rearguard.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "homepage": "https://github.com/Techintouch/rearguard",
  "bugs": {
    "url": "https://github.com/Techintouch/rearguard/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Techintouch/rearguard.git"
  },
  "author": {
    "name": "Dmitriy Borodin",
    "email": "dmitriy@borodin.site",
    "url": "http://borodin.site"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "mocha -R spec -r ts-node/register test/**/*.ts",
    "test:watch": "mocha -R spec -w --watch-extensions ts -r ts-node/register test/**/*.ts",
    "test:coverage": "nyc --reporter=html --extension .ts npm test",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'bin/**/*.ts' 'test/**/*.ts' --fix",
    "pretest": "npm run lint",
    "precommit": "npm test",
    "prepublishOnly": "npm test && npm run build && sh prepublish.sh"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer": "7.2.5",
    "chalk": "2.3.0",
    "chokidar": "2.0.0",
    "clean-webpack-plugin": "0.1.17",
    "compression": "1.7.1",
    "copy": "0.3.1",
    "cross-spawn": "6.0.0",
    "css-loader": "0.28.9",
    "del": "3.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "glob": "7.1.2",
    "hard-source-webpack-plugin": "0.5.16",
    "html-webpack-plugin": "2.30.1",
    "joi": "13.1.1",
    "mkdirp": "0.5.1",
    "node-object-hash": "1.3.0",
    "pleeease-filters": "4.0.0",
    "postcss-color-function": "4.0.1",
    "postcss-custom-media": "6.0.0",
    "postcss-custom-properties": "6.2.0",
    "postcss-custom-selectors": "4.0.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-import": "11.0.0",
    "postcss-initial": "2.0.0",
    "postcss-loader": "2.1.0",
    "postcss-media-minmax": "3.0.0",
    "postcss-selector-matches": "3.0.1",
    "postcss-selector-not": "3.0.1",
    "style-loader": "0.19.1",
    "ts-loader": "3.3.0",
    "ts-node": "4.1.0",
    "tslib": "1.9.0",
    "tslint": "5.9.0",
    "tslint-loader": "3.5.3",
    "typed-css-modules": "0.3.1",
    "typescript": "2.6.2",
    "typings-for-css-modules-loader": "1.7.0",
    "uglifyjs-webpack-plugin": "1.1.8",
    "webpack": "3.10.0",
    "webpack-bundle-analyzer": "2.9.2",
    "webpack-dev-server": "2.11.1",
    "workbox-webpack-plugin": "2.1.2"
  },
  "devDependencies": {
    "@types/assets-webpack-plugin": "3.5.0",
    "@types/chai": "4.1.1",
    "@types/chokidar": "1.7.4",
    "@types/clean-webpack-plugin": "0.1.0",
    "@types/compression": "0.0.35",
    "@types/del": "3.0.0",
    "@types/extract-text-webpack-plugin": "3.0.0",
    "@types/glob": "5.0.34",
    "@types/html-webpack-plugin": "2.30.1",
    "@types/joi": "13.0.5",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "2.2.45",
    "@types/node": "8.5.3",
    "@types/webpack": "3.8.3",
    "@types/webpack-bundle-analyzer": "2.9.0",
